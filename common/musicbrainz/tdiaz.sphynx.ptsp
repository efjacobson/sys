  \n$set(_separateAlbum,0) $noop(Albums in SubDirectory as well)\n$noop(\u2605 ~/Music/A/Artist/AlbumTitle,\
  \ ~/Music/A/Artist/Albums/AlbumTitle \u2605)\n\n$set(_showTrackArtistSeparation,0)\
